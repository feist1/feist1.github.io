page{
    width: 100%;
    height: 100%;

}
.focus{
    width: 100%;
}
.focus image{
    width: 100%;
    height: 100%;
}
.wx-swiper-dot::before{
   color: red
}
.newShop{
    
    font-size: 14px;
    position: relative;
    height: 30px;
    line-height: 30px;
    margin-left: 20px;
    margin-top: 20px;
}
.newShop::before{
    left: -15px;
    width: 10px;
    height: 100%;
    content: "";
    background: green;
    color: green;
    position: absolute;

}
.shopImg{
width:100px;
height:100px;
vertical-align: middle;
}
.info{
    display: block;
    margin-bottom: 20px
}
.shopInfoAndPrice{
    display: inline-block;
    margin-left: 20px;
}
.shop{
    padding: 5px;
    margin:20px 10px 0 10px;
    border: 1px solid darkgrey;
}